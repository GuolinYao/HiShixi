<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="285dp"
             android:layout_height="wrap_content"
             android:layout_gravity="center">

    <!--
       android:background="@drawable/dialog_frame_bg" 
       android:background="@drawable/quejiaoyuan"
    -->

    <ImageView
        android:layout_width="match_parent"
        android:layout_margin="15.5dp"
        android:layout_height="match_parent"
        android:background="@drawable/quejiaoyuan"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="15.5dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_dialog_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="23dp"
            android:gravity="center_horizontal"
            android:text="温馨提示"
            android:textColor="@color/main_font_color"
            android:textSize="17sp"/>

        <TextView
            android:id="@+id/tv_confirm_des"
            style="@style/list_marginLeftAndRight_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="20dp"
            android:layout_marginTop="25dp"
            android:text="你的建立完整度低于60%，很容易石城大海或被HR拒绝掉额"
            android:textColor="#999999"
            android:textSize="14sp"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#dddddd"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_title_cancel_btn"
                android:layout_width="0dp"
                android:layout_height="45dp"
                android:layout_weight="1"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/cancel_str"
                android:textColor="#00acf0"
                android:textSize="16sp"/>

            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="#dddddd"/>

            <TextView
                android:id="@+id/tv_title_confrim_btn"
                android:layout_width="0dp"
                android:layout_height="45dp"
                android:layout_weight="1"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/confirm_str"
                android:textColor="#00acf0"
                android:textSize="16sp"/>
        </LinearLayout>
    </LinearLayout>

    <!-- android:background="@drawable/white_round" -->

    <ImageButton
        android:id="@+id/cancel_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:background="@drawable/guanbi"
        android:contentDescription="@string/app_name"/>

</FrameLayout>